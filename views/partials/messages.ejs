<% if(typeof errors != "undefined") { %> <% errors.forEach(error => { %>
<div class="alert alert-warning alert-dismissible fade show" role="alert">
  <%= console.log("Errors:", errors) %> <%= error.msg %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<% }) %> <% } %> <% if (success_msg != "") { %>
<div class="alert alert-success alert-dismissible fade show" role="alert">
  <%= success_msg %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<% } %> <% if (error_msg != "") { %>
<div class="alert alert-warning alert-dismissible fade show" role="alert">
  <%= console.log("error_msg:", error_msg) %> <%= error_msg %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<% } %> <% if (error != "") { %>
<div class="alert alert-warning alert-dismissible fade show" role="alert">
  <%= console.log("Error:", error) %> <%= error %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<% } %>
